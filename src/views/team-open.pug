extends templates/layout

block vars

	- var title = 'Сборные - Национальные';

block content
	div.team
		div.container
			div.bread-crumbs
				a(href="").bread-crumbs__link.
					Сборные
				a(href="").bread-crumbs__link.
					Национальные
				span.
					Мужская сборная России, мировая лига на 2012 г
		div.container.container_main
			+sidebar(true)
			div.content
				h3.mb1.
					Мужская сборная России, мировая лига на 2012 г
				div.container__row
					div.container__col
						img(src="img/team-open-main-img.png", alt="")
					div.container__col
						div.note-block.mb0.mt0
							div.note-block__item.note-block__item_blue.team__note-block-head.
								Тренерский состав
						table.main-table.team__general-table
							tr.main-table__row.team__general-table-row
